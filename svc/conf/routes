# Routes
# This file defines all application routes (Higher priority routes first)

# NOTE***** Static content should be listed at the end of the file******

# ~~~~

# DYNAMIC CONTENT

GET         /project        controllers.resource.ProjectsController.list()
GET         /project/:id    controllers.resource.ProjectsController.find(id: Long)
POST        /project        controllers.resource.ProjectsController.create()
PUT         /project        controllers.resource.ProjectsController.update()
DELETE      /project/:id    controllers.resource.ProjectsController.delete(id: Long)


#STATIC CONTENT

# Home page
GET         /               controllers.Assets.at(path="/public/webapp", file="index.html")

# Map static resources from the /public folder to the /assets URL path
GET         /*file          controllers.Assets.versioned(path="/public/webapp", file: Asset)




